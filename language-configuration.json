{
    "comments": {
        "lineComment": "//",
        "blockComment": ["/*", "*/"]
    },
    "brackets": [
        ["[", "]"],
        ["[[", "]]"],
        ["{", "}"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["[", "]"],
        ["[[", "]]"],
        ["{", "}"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        ["**", "**"],
        ["__", "__"],
        ["^^", "^^"]
    ],
    "surroundingPairs": [
        ["[", "]"],
        ["[[", "]]"],
        ["{", "}"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        ["**", "**"],
        ["__", "__"],
        ["^^", "^^"]
    ],
    "folding": {
        "markers": {
            "start": "^\\s*-\\s",
            "end": "^\\s*(?=-\\s|$)"
        }
    },
    "wordPattern": "(?:\\^)?[\\w#\\[\\]]+",
    "onEnterRules": [
        {
            "beforeText": "^\\s*-\\s.*$",
            "action": {
                "indent": "indent"
            }
        }
    ]
}