{
    "name": "tana-paste",
    "displayName": "Tana Paste",
    "description": "Syntax highlighting for Tana Paste format",
    "version": "0.0.1",
    "publisher": "andrew-256",
    "repository": {
        "type": "git",
        "url": "https://github.com/andrew-256/tana-paste.git"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tanapaste",
            "aliases": ["Tana Paste", "tanapaste"],
            "extensions": [".tanapaste"]
        }],
        "grammars": [{
            "language": "tanapaste",
            "scopeName": "source.tanapaste",
            "path": "./syntaxes/tanapaste.tmLanguage.json"
        }],
        "snippets": [{
            "language": "tanapaste",
            "path": "./snippets/tanapaste.json"
        }]
    },
    "license": "MIT"
}